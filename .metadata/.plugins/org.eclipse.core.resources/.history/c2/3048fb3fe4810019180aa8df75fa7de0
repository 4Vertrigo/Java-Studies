package cursojava.exercicios.lista9;

public class Exercicio2 {

	public static void main(String[] args) {

		System.out.println(Calculadora.soma(5, 4));
		System.out.println(Calculadora.subtracao(10, 20));
		System.out.println(Calculadora.multipicacao(5, 6));
		System.out.println(Calculadora.divisao(10, 3));
		System.out.println(Calculadora.potencia(2,0));
	}

}
