package cursojava.aulas.aula5;

public class OperadoresAritmeticos {

	public static void main(String[] args) {
		
		int resultado = 1 + 2;
		System.out.println("" + resultado);
		
		int resultado = resultado - 1;
		System.out.println("" + resultado);
		
		int resultado = resultado * 2;
		System.out.println("" + resultado);
		
		int resultado / 2;
		System.out.println("" + resultado);
		
		int resultado % 7;
		System.out.println("" + resultado);
	}

}
