package cursojava.aulas.aula16;

public class Carro {
	
	//atributos
	String marca;
	String modelo;
	int numeroPassageiros;
	double capCombustivel;
	double consumoCombustivel;

	void exibirAutonomia(){
		System.out.println("A autonomia do carro eh: " + (capCombustivel * consumoCombustivel) + " km");
	}
}
