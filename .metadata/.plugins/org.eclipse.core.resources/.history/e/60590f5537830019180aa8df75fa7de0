package cursojava.exercicios.lista13.exercicio1;

public class Lista13 {
	
	public static void main(String[] args) {
		
		FiguraGeometrica[] figuras = new FiguraGeometrica[6];
		
		figuras[0] = new Quadrado(5.0); 
		figuras[1] = new Triangulo(5.0, 5.0);
		figuras[2] = new Circulo(5.0);
		figuras[3] = new Cubo(5.0);
		figuras[4] = new Cilindro(5.0, 5.0);
		figuras[5] = new Piramide(5.0, 5.0, 5.0, figuras[0], 5.0);
		
		/*
		Quadrado quadrado = new Quadrado(5.0);
		Triangulo triangulo = new Triangulo(5.0, 5.0);
		Circulo circulo = new Circulo(5.0);*/
		
		
	}

}
