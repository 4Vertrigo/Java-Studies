package cursojava.aulas.aula3;

public class VariaveisInteiras {

	
		public static void main(String[] args){
			
			byte idade1 = 20; //1 byte
			short idade2 = 21; //2 bytes
			int idade3 = 22; //4 bytes
			long idade4 = 23; //8 bytes
			
			System.out.println("Idade 1 = " + idade1);
			System.out.println("Idade 1 = " + idade2);
			System.out.println("Idade 1 = " + idade3);
			System.out.println("Idade 1 = " + idade4);
		}
}
