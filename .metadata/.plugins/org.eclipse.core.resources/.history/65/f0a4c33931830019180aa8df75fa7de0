package cursojava.exercicios.lista13.exercicio1;

public class Cilindro extends Figura3D {

	private Circulo circuloBase;
	private double altura;
	
	public Cilindro() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Cilindro(Circulo circuloBase, double altura) {
		super();
		this.circuloBase = circuloBase;
		this.altura = altura;
	}

	public Figura2D getCirculoBase() {
		return circuloBase;
	}

	public void setCirculoBase(Circulo circuloBase) {
		this.circuloBase = circuloBase;
	}

	public double getAltura() {
		return altura;
	}

	public void setAltura(double altura) {
		this.altura = altura;
	}

	@Override
	public double DimensaoVolumetrica() {
		return  * getAltura();
	}

}
