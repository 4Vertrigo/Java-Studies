package cursojava.aulas.aula2;

public class Variaveis {

	public static void main(String[] args) {
		
		//convercao Java
		int idade = 20;
		String nome = "Marco";
		String nomeDoMeuCachorro = "Dolly"; //O Java tem a convercao de ser camelCase
		
		//aceito mas nao utilizado
		//int _idade;
		//int $idade;
		
		
		System.out.println("Idade = " + idade);
		System.out.println("Nome = " + nome);
		System.out.println("Nome do cachorro = " + nomeDoMeuCachorro);
	}

}
