package courseADTs.vector.exercises.exercise1;

public class Main {

	public static void main(String[] args) {
		
		List<Integer> vector = new List<Integer>();
		
		vector.add(1);
		vector.add(2);
		vector.add(3);
	}
	
	public static void exists(List<Integer> vector, int pos)
	{
		if(vector.search(pos))
			System.out.println("Element exists in array");
		else
			System.out.println("Element doesn't exist in array");
	}
	
	

}
