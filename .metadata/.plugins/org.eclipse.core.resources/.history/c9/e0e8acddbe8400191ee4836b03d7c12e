package courseADTs.ADTs.vetor;

import java.util.ArrayList;

public class Native {

	public static void main(String[] args) {

		ArrayList<Integer> arrayList = new ArrayList<Integer>();
		
		arrayList.add(1);
		arrayList.add(2);
		arrayList.add(3);
		
		System.out.println(arrayList);
		
		arrayList.add(0, 0);
		
		System.out.println(arrayList);
	}

}
