package courseADTs.stack;

import courseADTs.base.StaticStruct;

public class Stack<T> extends StaticStruct<T> {

	public Stack() {
		super();
	}
	
	public Stack(int capacity) {
		super(capacity);
	}
	
	public void push(T element) {
		super.add(element);
	}
	
	public T pop() {
		T element = this.elements[super.size - 1];
		super.size--;
		
		return element;
	}
	
	public T top() {
		
		if(super.isVoid())
			return null;
		
		return this.elements[super.size - 1];
	}
}
